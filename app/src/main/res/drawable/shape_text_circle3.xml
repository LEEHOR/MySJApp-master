<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    android:useLevel="false">
    <solid android:color="#ffffff" />
    <!-- 圆角 -->
    <corners android:radius="360dp" />
